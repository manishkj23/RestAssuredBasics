package BasicsProgramming;

public class FindDigitsInArray {

	public static void main(String[] args) 
	{


		Object[] array = {'A', 12, 'C', 2, 1, 'W', 'V', 12};

        System.out.println("Digits in the array:");
        for (Object element : array) {
            if (element instanceof Integer) {
                System.out.println(element);
            }
        }
        
        for (Object element1 : array) {
            if (element1 instanceof Character) {
                System.out.println(element1);
            }
        }
        
        String s1="New";
        s1.concat("Delhi");
        System.out.println("Final Output is : " + s1);
        
        System.out.println("****************New Program***********");
        
        String str = "welcome to india";
        int countA =0, countE =0, countI =0, countO =0, countU =0;
        
        for ( int i=0; i<str.length();i++)
        {
        	char c = str.charAt(i);    
        	if(c == 'a' || c == 'e' || c == 'i' ||c == 'o' ||c == 'u')
        	{
       
        switch(c) {
            
            case 'a': 
            	countA++;
            	break;
            	
            case 'e':
            	countE++;
            	break;
            	
            case 'i':
            	countI++;
            	break;
            	
            case 'o':
            	countO++;
            	break;
            	
            case 'u':
            	countU++;
            	break;
            default:
            	System.err.println("no vowel present");
            	
//            char c = str.charAt(i);
//            if(c == 'a' || c == 'e' || c == 'i' ||c == 'o' ||c == 'u')
//            {
//                count ++;
//            }
            	
	}
        	}
        System.out.println("Total count :" + c + ":" + countA );
        System.out.println("Total count :" + c + ":" + countE );
        System.out.println("Total count :" + c + ":" + countI );
        System.out.println("Total count :" + c + ":" + countO );
        System.out.println("Total count :" + c + ":" + countU );
  }
	}

}
