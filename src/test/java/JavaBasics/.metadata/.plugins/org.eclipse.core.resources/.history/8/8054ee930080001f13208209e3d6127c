package StringsPrograms;

public class ChangeCaseOfEachAlternateWord {

	public static void main(String[] args) {
		
		String s = "A RED SEED";
		String w = "";
		s = s + " ";
		String ns = "";
		for(int i=0;i<s.length();i++)
		{
			char c = s.charAt(i);
			if(c != ' ') 
			{
				w = w + c;
			}
			else
			{
				
				
				
				w = "";
			}	
		}
		System.out.println(ns);

	}

}
