package BasicsProgramming;

public class FindDigitsInArray {

	public static void main(String[] args) 
	{


		Object[] array = {'A', 12, 'C', 2, 1, 'W', 'V', 12};

        System.out.println("Digits in the array:");
        for (Object element : array) {
            if (element instanceof Integer) {
                System.out.println(element);
            }
        }
        
        for (Object element1 : array) {
            if (element1 instanceof Character) {
                System.out.println(element1);
            }
        }
        
        String s1="New";
        s1.concat("Delhi");
        System.out.println("Final Output is : " + s1);
        
        System.out.println("****************New Program***********");
        
        String str = "welcome to india";
        int count =1;
        
        for ( int i=0; i<str.length();i++)
        {
        	char c = str.charAt(i);           
       
        switch(c):
            
            case 'a': 
            	count++;
            	break;
            	
            case 'e':
            	count++;
            	break;
            	
            case 'i':
            	count++;
            	break;
            	
            case 'o':
            	count++;
            	break;
            	
            case 'u':
            	count++;
            	break;
            	
//            char c = str.charAt(i);
//            if(c == 'a' || c == 'e' || c == 'i' ||c == 'o' ||c == 'u')
//            {
//                count ++;
                System.out.println("Total count :" + c + ":" + count );
//            }
            
        }
        
    }
	
	}

}
